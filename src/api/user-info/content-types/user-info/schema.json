{
  "kind": "collectionType",
  "collectionName": "user_infos",
  "info": {
    "singularName": "user-info",
    "pluralName": "user-infos",
    "displayName": "User info",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "confirm_password": {
      "type": "password",
      "required": true
    },
    "otps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::otp.otp",
      "mappedBy": "user_info"
    },
    "isSubscribed": {
      "type": "boolean"
    },
    "wishlist": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "user_info"
    },
    "carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "mappedBy": "user_infos"
    }
  }
}
